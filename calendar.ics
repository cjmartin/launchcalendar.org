---
layout: calendar
---
{% for post in site.posts %}
BEGIN:VEVENT
UID:{{ post.created | date: "%Y%m%dT%H%M%SZ" }}@launchcalendar.org
CREATED:{{ post.created | date: "%Y%m%dT%H%M%SZ" }}
DTSTAMP:{{ post.updated | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ post.date | date: "%Y%m%dT%H%M%SZ" }}
DURATION:PT1H
SUMMARY:{{ post.title | replace: '\\', '\\\\' | replace: ';', '\\;' | replace: ',', '\\,' | replace: '\n', '\\n' }}
DESCRIPTION:{{ post.description | replace: '\\', '\\\\' | replace: ';', '\\;' | replace: ',', '\\,' | replace: '\n', '\\n' }}
LOCATION:{{ post.location | replace: '\\', '\\\\' | replace: ';', '\\;' | replace: ',', '\\,' | replace: '\n', '\\n' }}
{% if post.geo-lat and post.geo-lon %}GEO:{{ post.geo-lat }};{{ post.geo-lon }}
{% endif %}
BEGIN:VALARM
TRIGGER:-PT1H
DESCRIPTION:{{ post.title | replace: '\\', '\\\\' | replace: ';', '\\;' | replace: ',', '\\,' | replace: '\n', '\\n' }} is scheduled to ðŸš€ in one hour!
ACTION:DISPLAY
END:VALARM
END:VEVENT
{% endfor %}
