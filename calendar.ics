---
layout: nil
---
BEGIN:VCALENDAR
PRODID:-//launchcalendar.org//Launch Calendar//EN
VERSION:2.0
NAME:Launch Calendar
X-WR-CALNAME:Launch Calendar
{% for post in site.posts %}
BEGIN:VEVENT
UID:{{ post.created | date: "%Y%m%dT%H%M%SZ" }}@launchcalendar.org
CREATED:{{ post.created | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ post.date | date: "%Y%m%dT%H%M%SZ" }}
DURATION:PT1H
SUMMARY:{{ post.title }}
LOCATION:{{ post.location | replace:',','\,' }}
GEO:{{ post.geo-lat }};{{ post.geo-lon }}
END:VEVENT
{% endfor %}
END:VCALENDAR
