---
layout: calendar
---
{% for post in site.posts %}
BEGIN:VEVENT
UID:{{ post.created | date: "%Y%m%dT%H%M%SZ" }}@launchcalendar.org
CREATED:{{ post.created | date: "%Y%m%dT%H%M%SZ" }}
DTSTAMP:{{ post.updated | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ post.date | date: "%Y%m%dT%H%M%SZ" }}
DURATION:PT1H
SUMMARY:{% include ical_escape.html value=post.title -%}
DESCRIPTION:{% include ical_escape.html value=post.description -%}
LOCATION:{% include ical_escape.html value=post.location -%}
{% if post.geo-lat and post.geo-lon %}GEO:{{ post.geo-lat }};{{ post.geo-lon }}
{% endif %}
BEGIN:VALARM
TRIGGER:-PT1H
DESCRIPTION:{% include ical_escape.html value=post.title -%} is scheduled to ðŸš€ in one hour!
ACTION:DISPLAY
END:VALARM
END:VEVENT
{% endfor %}
